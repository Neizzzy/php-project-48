{
    "name": "hexlet/code",
    "description": "gendiff package",
    "license": "MIT",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files": [
            "src/Differ.php",
            "src/Utils.php",
            "src/Parsers.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Json.php",
            "src/Formatters.php"
        ]
    },
    "require": {
        "docopt/docopt": "^1.0",
        "lstrojny/functional-php": "^1.17",
        "symfony/yaml": "^7.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.12",
        "phpunit/phpunit": "^12.1"
    }
}
